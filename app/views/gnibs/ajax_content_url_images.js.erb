$('#content_url_images').html('<%= escape_javascript(render  :partial=>"gnibs/content_url") %>');
$('#373737_busy_image').hide();
<% if @title && @title.size > 0 %>
$('#title_field').val('<%= @title %>')
<% end %>
<% if @video %>
$('#is_video_field').val('true')
<% end %>
<% if @image_sources.length > 0 %>
<% p "#{@image_sources.length}" %>
  $('#content_url_images').show();
  $('#file_upload_option').hide();
<% end %>

 
