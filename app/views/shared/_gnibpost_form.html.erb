<%= form_for(@gnib) do |f|%>

<%= f.text_field :title, placeholder: "title" %>
<%= f.text_field :description, placeholder: "description, less than 77 characters" %>
<%= f.text_field :landmark, placeholder: "landmark" %>
<%= f.file_field :image, placeholder: "image" %>

<%= f.submit "Gnibl",class:"btn btn-large btn-primary" %>
<% end %>
